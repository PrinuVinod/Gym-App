<body>
    <!-- Page Preloader -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Offcanvas Menu Section Begin -->
    <div class="offcanvas-menu-overlay"></div>
    <div class="offcanvas-menu-wrapper">
        <div class="canvas-close">
            <i class="fa fa-close"></i>
        </div>
        <div class="canvas-search search-switch">
            <i class="fa fa-search"></i>
        </div>
        <nav class="canvas-menu mobile-menu">
            <ul>
                <li><a href="/">Home</a></li>
                <li class="active"><a href="/selectuser">Login?</a></li>
            </ul>
        </nav>
        <div id="mobile-menu-wrap"></div>
        <div class="canvas-social">
            <a href="https://www.instagram.com/kgf.the_oasis_patharam/"><i class="fa fa-instagram"></i></a>
            <a href="/selectuser"><i class="fa fa-user"></i></a>
        </div>
    </div>
    <!-- Offcanvas Menu Section End -->

    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-3">
                    <div class="logo">
                        <a href="/">
                            <img style="width: 100px;" src="/img/logo.png" alt="">
                        </a>
                    </div>
                </div>
                <div class="col-lg-6">
                    <nav class="nav-menu">
                        <ul>
                            <li><a href="/">Home</a></li>
                            <li class="active"><a href="/selectuser">Login?</a></li>
                        </ul>
                    </nav>
                </div>
                <div class="col-lg-3">
                    <div class="top-option">
                        <div class="to-search search-switch">
                            <a
                                href="https://www.google.com/maps/place/KGF+MULTI+GYM/@9.0741904,76.6037215,17z/data=!3m1!4b1!4m6!3m5!1s0x3b06050c78c8af15:0x83ac2a156edbcc07!8m2!3d9.0741904!4d76.6062964!16s%2Fg%2F11v4_1_3vm?entry=ttu"><i
                                    class="fa fa-search"></i></a>
                        </div>
                        <div class="to-social">
                            <a href="https://www.instagram.com/kgf.the_oasis_patharam/"><i
                                    class="fa fa-instagram"></i></a>
                            <a href="/selectuser"><i class="fa fa-user"></i></a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="canvas-open">
                <i class="fa fa-bars"></i>
            </div>
        </div>
    </header>
    <!-- Header End -->

    <!-- Profile Section Begin -->
    <section class="profile-section spad" style="margin-top: 50px; text-align: center;">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <span>Member Login</span>
                        <h2>Login to View Your Profile</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="profile-info">
                        <form action="/memberlogin" method="POST">
                            <label for="phone" style="color: white;">Phone Number:</label><br>
                            <input type="text" id="phone" name="phone" required> <br><br><br>

                            <label for="password" style="color: white;">Password:</label><br>
                            <input type="password" id="password" name="password" required><br><br>

                            <button type="submit" class="subbut">Login</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Profile Section End -->

    <!-- Footer Section Begin -->
    <footer class="footer-section" id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-2">
                    <div class="fs-about">
                        <div class="fa-logo">
                            <a href="#"><img src="/img/logo.png" alt=""></a>
                        </div>
                        <div class="fa-social">
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 offset-lg-1">
                    <div class="fs-widget">
                        <a
