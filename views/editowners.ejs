    <!-- Profile Section Begin -->
    <section class="profile-section spad" style="margin-top: 50px; text-align: center;">
        <div class="container">
            <section class="edit-form">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <h2 style="color: #f36100; margin-bottom: 20px;">Edit Member Details</h2>
                            <form action="/updateowner/<%= user._id %>" method="post">
                                <input type="text" name="name" value="<%= user.name %>" placeholder="Name"
                                    required><br><br>
                                <input type="text" name="phone" value="<%= user.phone %>" placeholder="Phone"
                                    required><br><br>
                                    <input type="text" name="age" value="<%= user.age %>" placeholder="age" required><br><br>
                                <button class="subbut" style="border-radius: 10px;"
                                    onclick="window.location.href='/ownerprofile'">Back</button>
                                <button type="submit" class="subbut" style="border-radius: 10px;">Update</button>
                            </form>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </section>
